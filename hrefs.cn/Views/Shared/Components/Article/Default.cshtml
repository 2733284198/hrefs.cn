@model IEnumerable<Article>
@foreach (var item in Model)
{
    <div class="site-item col-4" style="padding-left:10px;">
        <div style="float:left;"><img src="/list.png" style="height:20px;margin-right:10px;" /><a href="/article/@item.Id" style="color:#CD661D;">@Html.Raw(item.Title)</a></div>
    </div>
}