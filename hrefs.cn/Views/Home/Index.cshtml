@{
    Layout = null;
    var common = (IEnumerable<Link>)ViewBag.common;
    var info = (IEnumerable<Link>)ViewBag.info;
    var frontend = (IEnumerable<KeyValuePair<string, IGrouping<string, Link>>>)ViewBag.frontend;
    var backend = (IEnumerable<KeyValuePair<string, IGrouping<string, Link>>>)ViewBag.backend;
    var hot = (IEnumerable<KeyValuePair<string, IGrouping<string, Link>>>)ViewBag.hot;
    var other = (IEnumerable<KeyValuePair<string, IGrouping<string, Link>>>)ViewBag.other;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=0" />
    <meta name="keywords" content="上网导航，hrefs.cn，科技网址，网址导航，程序员自学教程网站，www.hrefs.cn，前端开发，后端开发，数据库，程序员网址导航，程序员常去的网站">
    <meta name="description" content="程序员日常使用的网站，Java Spring网站，网址导航，Asp.Net Core框架，JSON在线解释，Python网址，微服务大全，NodeJS框架，科技网站，Golang网址，程序员学习网站，Vue组件网址">
    <link href="https://cdn.bootcss.com/twitter-bootstrap/4.3.1/css/bootstrap.min.css" rel="stylesheet">
    <link href="/styles/site.css?201910061252" rel="stylesheet">
    <script src="https://cdn.bootcss.com/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdn.bootcss.com/twitter-bootstrap/4.3.1/js/bootstrap.min.js"></script>
    <title>程序员网址导航 - 程序员常用的网址大全</title>
</head>
<body>
    @await Html.PartialAsync("_header")
    <div id="main">
        <div style="">
            @await Html.PartialAsync("_common", common)
            @await Html.PartialAsync("_info", info)
            @await Component.InvokeAsync("Article", new { size = 15, skip = 0 })
            <div id="other">@await Html.PartialAsync("_group", other)</div>
            <div id="backend">@await Html.PartialAsync("_group", backend)</div>
            <div id="frontend">@await Html.PartialAsync("_group", frontend)</div>
            <div id="hot">@await Html.PartialAsync("_group", hot)</div>
        </div>
    </div>
    <div id="footer">
        @await Html.PartialAsync("_footer")
    </div>
</body>
</html>